<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/scrollView1">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:minWidth="25px"
        android:minHeight="25px">
        <TextView
            android:text="1: Cuales son sistemas operativos?"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="21.0dp" />
        <CheckBox
            android:text="Windows,LInux,mac Os"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx1"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="Ubuntu,Luna,BMware"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx2"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="VirtualBox,Oracle,Azure"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx3"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <CheckBox
            android:text="Todas las anteriores"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx4"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <TextView
            android:text="2: Que es JOptionPane?"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="21.0dp" />
        <CheckBox
            android:text="LLamar base de datos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx5"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="Abrir pagina web"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx6"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="Mostrar ventanas emergentes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx7"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <CheckBox
            android:text="Importar imagenes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx8"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <TextView
            android:text="3: Como llamar una tabla?"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="21.0dp" />
        <CheckBox
            android:text="SELECT * FROM NOMBRE_TABLA"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx9"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="SELECTO * FROM NOMBRE_TABLA "
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx10"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="SELEC * FRON NOMBRE_TABLA "
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx11"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <CheckBox
            android:text="FROM * SELECT NOMBRE_TABLA"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx12"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <TextView
            android:text="4: Con que se instala un sistema operativo?"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="21.0dp" />
        <CheckBox
            android:text="Flash"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx13"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="Flash boteada"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx14"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="iso"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx15"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <CheckBox
            android:text="Por la bios"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx16"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <TextView
            android:text="5: Cual es la diferencia de una clace y un objeto?"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="21.0dp" />
        <CheckBox
            android:text="Consiste en tener un método con el mismo nombre pero diferentes parámetros."
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx17"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="Podemos definirlo como la capacidad que tienen los objetos de comportarse de múltiples formas recurriendo a la herencia, en si consiste en hacer referencia a objetos de una clase que puede tomar comportamientos de objetos que son descendientes de esta."
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx18"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />

        <CheckBox
            android:text="Es la forma de proteger u ocultar las propiedades de un objetos, estableciendo los permisos y niveles de visibilidad, existen tres niveles de acceso para el encapsulamiento"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx19"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <CheckBox
            android:text="Podemos ver la clase con un modelo o plantilla y a un objeto es el instanciamiento de esa plantilla."
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cbx20"
            android:layout_marginLeft="29.5dp"
            android:layout_marginTop="14.5dp" />
        <Button
            android:text="Revisar Respuestas"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnRevisar1"
            android:layout_marginTop="20.0dp"
            android:layout_marginRight="23.5dp"
            android:layout_marginLeft="27.5dp" />



    </LinearLayout>
</ScrollView>
